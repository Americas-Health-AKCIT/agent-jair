{
    "family": "jair-autorizacoes-dev-task",
    "containerDefinitions": [
      {
        "name": "jair-autorizacoes-dev-container",
        "image": "dkr.ecr.us-east-1.amazonaws.com/jair-autorizacoes",
        "cpu": 256,
        "memory": 512,
        "essential": true,
        "environment": [
          { "name": "AMH_QDRANT_URL", "value": "PLACEHOLDER_AMH_QDRANT_URL" },
          { "name": "AMH_QDRANT_KEY", "value": "PLACEHOLDER_AMH_QDRANT_KEY" },
          { "name": "LANGFUSE_PUBLIC_KEY", "value": "PLACEHOLDER_LANGFUSE_PUBLIC_KEY" },
          { "name": "LANGFUSE_SECRET_KEY", "value": "PLACEHOLDER_LANGFUSE_SECRET_KEY" },
          { "name": "LANGFUSE_HOST", "value": "PLACEHOLDER_LANGFUSE_HOST" },
          { "name": "AMH_OPENAI_KEY", "value": "PLACEHOLDER_AMH_OPENAI_KEY" }
        ],
        "portMappings": [
          {
            "containerPort": 8501,
            "hostPort": 8501,
            "protocol": "tcp"
          }
        ]
      }
    ],
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::207567764107:role/ecsTaskExecutionRole"
  }